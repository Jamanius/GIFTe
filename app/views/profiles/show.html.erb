<h1><%= @user.profile_name%></h1>
<% if @gifts %>
  <% @gifts.each do |gift| %>
    <div class="well">
      <%= link_to gift.title, gift_path %><br>
      <em><%= gift.description %></em>
      <hr />
      <%= link_to time_ago_in_words(gift.created_at), gift_path(gift) %> ago
    </div>
  <% end %>
<% end %>